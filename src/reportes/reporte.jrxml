	<queryString>
		<![CDATA[SELECT
     ventas.`idVenta` AS ventas_idVenta,
     cliente.`NombreCli` AS cliente_NombreCli,
     ventas.`FechaVenta` AS ventas_FechaVenta,
     ventas.`MetodoPago` AS ventas_MetodoPago,
     ventas.`Total` AS ventas_Total
FROM
     `ventas` ventas INNER JOIN `cliente` cliente ON ventas.`idCliente` = cliente.`idCliente`
WHERE cliente.`NombreCli` LIKE "%$P!{nombreCli}%"]]>
	</queryString>
